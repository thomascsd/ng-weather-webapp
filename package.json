  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "dev:server": "nodemon ./dist/server/index.js",
    "dev:client": "ng serve --proxy-config proxy.conf.json -o",
    "deploy": "git push origin master && git push heroku master",
    "build": "npm run build:server && npm run build:client && build:cpy",
    "build:client": "ng build --prod",
    "build:server": "tsc --project server",
    "build:cpy": "cpy ./server/.env ./dist/server/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
